          "var virtualHost = pm.environment.get(\"virtualhost-default\");",
          "",
          "// Map the environment to the correct azure post fix",
          "var azurePostFix = null;",
          "switch(virtualHost) {",
          "    case 'api.easternbank.com':",
          "        azurePostFix = 'preprod';",
          "        break;",
          "    case 'api-readiness.easternbank.com':",
          "        azurePostFix = 'qa';",
          "        break;",
          "    case 'api-staging.easternbank.com':",
          "    case 'api-uat.easternbank.com':",
          "        azurePostFix = 'staging';",
          "        break;",
          "    default:",
          "        azurePostFix = 'integration';",
          "}",
          "",
          "pm.environment.set(\"azure-postfix\", azurePostFix);"
        ]
